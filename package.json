{
  "name": "legal-leads-crm",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "frontend",
    "worker",
    "shared"
  ],
  "scripts": {
    "dev:frontend": "npm --workspace frontend run dev",
    "dev:worker": "npm --workspace worker run dev",
    "build": "npm run build:shared && npm run build:worker && npm run build:frontend",
    "build:shared": "npm --workspace shared run build",
    "build:worker": "npm --workspace worker run build",
    "build:frontend": "npm --workspace frontend run build"
  }
}
